NODE_ENV=development
PORT=3001

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/custom_backend_db

# Exponentia API Configuration
EXPONENTIA_API_URL=https://api.exponentia.com
EXPONENTIA_API_KEY=your_api_key_here

# WhatsApp Bot Configuration
WHATSAPP_BOT_PHONE=+1234567890

# Redis Configuration (optional, for caching)
REDIS_URL=redis://localhost:6379

# Logging Configuration
LOG_LEVEL=info

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3001,https://yourdomain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
